        <script>
            var score=document.getElementById("score").value;
            score=JSON.parse(score);
            // console.log(score);
            var dd=score.January;
            makedb(dd);
            function makedb(dd) {
                var xValues=[],yValues=[];
                for(var i=0;i<dd.length;i++){
                    xValues.push(i+1);
                    yValues.push(parseInt(dd[i]));
                }
                makeChart(xValues,yValues);    
            }
            
            // var xValues = [50,60,70,80,90,100,110,120,130,140,150];
            // var yValues = [7,8,8,9,9,9,10,11,14,14,15];
            function makeChart(xValues,yValues){
                new Chart("myChart", {
                type: "line",
                data: {
                    labels: xValues,
                    datasets: [{
                    fill: false,
                    lineTension: 0,
                    backgroundColor: "rgba(0,0,255,1.0)",
                    borderColor: "rgba(0,0,255,0.1)",
                    data: yValues
                    }]
                },
                options: {
                    legend: {display: false},
                    scales: {
                    yAxes: [{ticks: {min: 0, max:60}}],
                    }
                }
                });
            }
            
            function myfunc(){
                var mon=document.getElementById("months").value;
                switch(mon) {
                    case "January":
                        var dd=score.January;
                        makedb(dd);
                        break;
                    case "February":
                        var dd=score.February;
                        makedb(dd);
                        break;
                    case "March":
                        var dd=score.March;
                        makedb(dd);
                        break;
                    case "April":
                        var dd=score.April;
                        makedb(dd);
                        break;
                    case "May":
                        var dd=score.May;
                        makedb(dd);
                        break;
                    case "June":
                        var dd=score.June;
                        makedb(dd);
                        break;
                    case "July":
                        var dd=score.July;
                        makedb(dd);
                        break;
                    case "August":
                        var dd=score.August;
                        makedb(dd);
                        break;
                    case "September":
                        var dd=score.September;
                        makedb(dd);
                        break;
                    case "October":
                        var dd=score.October;
                        makedb(dd);
                        break;
                    case "November":
                        var dd=score.November;
                        makedb(dd);
                        break;
                    default:
                        var dd=score.December;
                        makedb(dd);
                        break;
                }
            }

            var leader=document.getElementById("leader").value;
            leader=JSON.parse(leader);
            leader.sort((a,b)=>{
                return  b.score - a.score;
            })
            // console.log(leader);
            var names=[],count=[],marks=[];
            leader.forEach(std=>{
                names.push(std.fname);
                count.push(std.count);
                marks.push(std.score);
            })
            // console.log(names);
            // console.log(marks);
            // console.log(count);

            var tbody=document.getElementById('tb1');
            tbody.innerHTML="";
            for(var i=0;i<names.length;i++){
                const tr = tbody.insertRow();
                const td1 = tr.insertCell();
                td1.appendChild(document.createTextNode(i+1));
                
                const td2 = tr.insertCell();
                td2.appendChild(document.createTextNode(names[i]));
                
                const td3 = tr.insertCell();
                td3.appendChild(document.createTextNode(count[i]));
                
                const td4 = tr.insertCell();
                td4.appendChild(document.createTextNode(marks[i]));
            }

            </script>






